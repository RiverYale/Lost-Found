}

.zhezhao .zhezhaoContent1 .watch {
  background: #4d86f8;
  width: 75%;
  height: 70rpx;
  font-size: 30rpx;
  text-align: center;
  color: #fff;
  border-radius: 98rpx;
  line-height: 70rpx;
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-top: auto;
