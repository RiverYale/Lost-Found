/* pages/lost.wxss */
@import "../../colorui/main.wxss";
@import "../../colorui/icon.wxss";

.btn {
  display: flex;
  flex-direction: row;
}

.container {
  width: 100%;
  height: 100%;
}

.swiper_lost {
  width: 100%;
  height: 200rpx;
}

.slide-image {
  width: 100%;
}

.btn_1 {
  margin-left: 10px;
  height: 60rpx;
  width: 150rpx;
  line-height: 60rpx;
  border-radius: 30px;
  color: #30ab42;
  background: #f2f3f5;
  font-size: 14px;
  font-weight: 800;
  align-items: center;
  justify-content: center;
  text-align-last: justify;
}

.btn_1::after {
  border-radius: 60px;
  border: none;
}

.container_cs {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-top: 3px;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 0px 0px rgba(229, 229, 229, 1);
}

.lost_search {
  margin-left: 10px;
  height: 60rpx;
  width: 720rpx;
  font-weight: 300;
  background: #f2f3f5;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
}

.searchicon {
  margin: 3% 3% auto;
}

.nav_right {
  /* display: flex;
  flex-direction: row; *//* display: inline-block;
  width: 100%; */
  height: 100%;
  padding: 20rpx;
  margin-left: 5px;
  background: #fff;
  box-sizing: border-box;
}

.nav_right .nav_right_items {
  display: inline-block;
  width: 47%;
  height: 300rpx;
  text-align: center;
  margin-bottom: 50rpx;
  margin-right: 10px;
}

.nav_right .nav_right_items image {
  width: 100%;
  height: 250rpx;
  border-radius: 15rpx;
}

.nav_right .nav_right_items text {
  display: block;
  margin-top: 0rpx;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: left;
  color: #6f6a6a;
  font-weight: 600;
}

.nav_left {
  display: inline-block;
  width: 23%;
  height: 80%;
  background: #fff;
  border-right: 2px solid #f2f3f5;
  text-align: center;
  position: absolute;
  z-index: 100;
  /* border-radius: 15rpx; */
}

.nav_left .nav_left_items {
  display: flex;
  flex-direction: row;
  height: 45px;
  line-height: 30px;
  padding: 6px 0;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
}

.nav_left .nav_left_items.active {
  background: #f2f3f5;
  width: 170rpx;
  border-radius: 0 12rpx 12rpx 0;
  color: #30ab42;
}

.nav_left .nav_left_items .nav {
  width: 100%;
}

.nav_left .nav_left_items .nav_ {
  margin-right: 8rpx;
  width: 91%;
}

.tixing {
  margin-top: 8px;
  margin-left: -5px;
  height: 12px;
  width: 3px;
  border-radius: 50rpx 0 0 50rpx;
  background: #30ab42;
}

.zhezhao {
  height: 100%;
  width: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 200;
  top: 0;
  left: 0;
}

.zhezhao1 {
  height: 100%;
  width: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 200;
  top: 0;
  left: 0;
}

.zhezhaoContent {
  padding: 20rpx 0;
  width: 70%;
  height: 260rpx;
  background: #fff;
  border-radius: 5rpx;
  margin: 55% auto;
  justify-content: space-around;
  align-items: center;
  position: relative;
}

.zhezhao1 .close {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  margin-left: 103%;
  margin-top: -17%;
}

.zhezhao1 .zhezhaoContent .watch {
  background: #4d86f8;
  width: 50%;
  height: 25%;
  font-size: 35rpx;
  text-align: center;
  color: #fff;
  border-radius: 98rpx;
  line-height: 65rpx;
  margin-top: 20px;
}

.completeinfo {
  margin-top: 45px;
  color: #6f6a6a;
  text-align: center;
  font-size: 30rpx;
}

.zhezhao1 .zhezhaoContent .tishi {
  width: 100%;
  height: 50%;
  text-align: center;
  line-height: 56px;
  font-size: 35rpx;
  font-weight: 400;
  border-bottom: 1px solid #f2f3f5;
}

.zhezhao .zhezhaoContent1 {
  padding: 15px 0 0 0;
  width: 70%;
  height: 700rpx;
  background: #fff;
  border-radius: 10rpx;
  margin: 20% auto;
}

.zhezhao .zhezhaoContent1 .zhanshi {
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 80%;
  height: 45%;
