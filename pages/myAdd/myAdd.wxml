<!--pages/myAdd/myAdd.wxml-->
<view class='title'>我已发布的招领信息</view>
<view wx:for='{{dataArray}}' class='item-container' wx:key='{{index}}'>
    <view class='info-container'>
        <text class='info-type'>{{item.type}}</text>
        <text class='info-time'>{{item.time}}</text>
    </view>
    <button class='cancelBtn' bindtap='cancel' data-index='{{index}}'>撤销</button>
</view>
<view class='line-hor' />